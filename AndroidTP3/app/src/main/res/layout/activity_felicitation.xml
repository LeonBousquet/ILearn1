<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".FelicitationActivity"
    android:orientation="vertical">

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:text="FELICITATIONS !!" />

    <Button
        android:id="@+id/content_felicitation_nouvtable"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:text="CHOISIR UNE AUTRE TABLE"
        android:onClick="autreTable"
        />

    <Button
        android:id="@+id/content_felicitation__nouvexo"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:text="CHOISIR UN AUTRE EXERCICE"
        android:onClick="autreExo"
        />


</LinearLayout>